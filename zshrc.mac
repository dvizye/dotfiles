# Start MATLAB with Java 9 to fix blurriness
export MATLAB_JAVA='/Library/Java/JavaVirtualMachines/jdk1.8.0_20.jdk/Contents/Home/jre'

# Turn on LSCOLORS for Mac
export CLICOLOR=1
export LSCOLORS=gxBxhxDxfxhxhxhxhxcxcx

# Set up drake for MATLAB
export PKG_CONFIG_PATH=$PKG_CONFIG_PATH:/opt/X11/lib/pkgconfig
