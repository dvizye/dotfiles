[user]
    name = David Ye
    email = dvizye@gmail.com
[alias]
    co = checkout
    ci = commit
    st = status
    br = branch
    hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
    type = cat-file -t
    dump = cat-file -p
    ctags = !.git/hooks/ctags
    amend = commit -a --amend --no-edit
[init]
    templatedir = ~/.git_template
[diff]
    tool = vimdiff
[push]
	default = current
[core]
    editor = vim
    pager  = less
[mergetool "fugitive"]
	cmd = vim -f -c \"Gdiff\" \"$MERGED\"
[merge]
	tool = fugitive
